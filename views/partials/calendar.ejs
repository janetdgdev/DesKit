<div id="calendar" class="container-fluid vh-100 text-center mx-auto mt-5">
  <div class="row vw-100 h-100 g-2">
    <div class="col-8 text-light">
      <div class="card p-1 h-75">
        <h3 class="card-header" id="monthAndYear"></h3>
        <table class="table table-bordered table-responsive-sm" id="calendar">
          <thead>
          <tr>
            <th>Sun</th>
            <th>Mon</th>
            <th>Tue</th>
            <th>Wed</th>
            <th>Thu</th>
            <th>Fri</th>
            <th>Sat</th>
          </tr>
          </thead>
        <tbody id="calendar-body">
        </tbody>
        </table>
        <div class="form-inline p-4">
          <button class="btn btn-outline-primary col-2" id="previous" onclick="previous()">Previous</button>
          <button class="btn btn-primary col" id="previous" onclick="goToToday()">Today</button>
          <button class="btn btn-outline-primary col-2" id="next" onclick="next()">Next</button>
        </div>
        <span>Jump to:</span>
        <div class="row g-1 px-5 mx-5">
          <div class="col">
            <form class="form">
              <label for="month"></label>
              <select class=" col form-control " name="month" id="month" onchange="jump()">
                <option value=0>Jan</option>
                <option value=1>Feb</option>
                <option value=2>Mar</option>
                <option value=3>Apr</option>
                <option value=4>May</option>
                <option value=5>Jun</option>
                <option value=6>Jul</option>
                <option value=7>Aug</option>
                <option value=8>Sep</option>
                <option value=9>Oct</option>
                <option value=10>Nov</option>
                <option value=11>Dec</option>
              </select>
            </form>
          </div>
          <div class="col">
            <form class="form">
              <label for="year"></label><select class="form-control" name="year" id="year" onchange="jump()">
                <option value=2010>2010</option>
                <option value=2011>2011</option>
                <option value=2012>2012</option>
                <option value=2013>2013</option>
                <option value=2014>2014</option>
                <option value=2015>2015</option>
                <option value=2016>2016</option>
                <option value=2017>2017</option>
                <option value=2018>2018</option>
                <option value=2019>2019</option>
                <option value=2020>2020</option>
                <option value=2021>2021</option>
                <option value=2022>2022</option>
                <option value=2023>2023</option>
                <option value=2024>2024</option>
                <option value=2025>2025</option>
                <option value=2026>2026</option>
                <option value=2027>2027</option>
                <option value=2028>2028</option>
                <option value=2029>2029</option>
                <option value=2030>2030</option>
              </select>
            </form>
          </div>
      </div>
      <div class="row g-1 my-3 mx-auto">
        <form class="row row-cols-lg-auto g-3 m-auto align-items-center" style="flex-wrap: nowrap;">
          <div class="col-4 m-0">
            <label class="visually-hidden" for="eventInput">Add Event</label>
            <div class="input-group">
              <input type="text" class="form-control" id="eventInput" placeholder="">
            </div>
          </div>
          <div class="col-4 m-auto">
            <button type="submit" onclick="addEvent()" class="btn btn-primary">Add</button>
          </div>
        </form>
      </div>
    </div>
    </div>
    <div class="col-4 text-light">
      <div class="card p-1">
        <h3 class="card-header" id="plannerDate"></h3>
        <table class="table table-bordered table-responsive-sm w-auto" id="calendar">
          <thead>
            <tr><th class="p-2 text-center">Planner</tr>
          </thead>
        <tbody id="planner-body">
        </tbody>
        </table>
      </div>
  </div>
  </div>
</div>